# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type UserDTO {
  id: ID!
  age: Int!
  username: String!
}

type Query {
  me: UserDTO!
}

input CreateUserDTO {
  age: Int!
  username: String!
  password: String!
  passwordConfirm: String!
}

type Mutation {
  signUp(user: CreateUserDTO!): UserDTO!
}
